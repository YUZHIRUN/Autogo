/*list widget*/
QListWidget#tool_item {
border:1px solid rgb(213, 213, 213);
border-radius:10px;
outline:0px;
	background-color: rgb(240, 240, 240);

}
QListWidget#tool_item::item{
margin:20px;
padding:2px 4px;
height:35px;
border:1px solid rgb(213, 213, 213);
border-radius:10px;
}
QListWidget#tool_item::item:hover{
	background-color: rgba(190, 227, 255,200);
}
QListWidget#tool_item::item:selected{
	border-width:0;
	background-color: rgba(190, 227, 255,255);
}
QListWidget#tool_item::item::selected:active{
	border: 0px solid gray;

}

/*list widget*/
QTabWidget QListWidget {
border:1px solid rgb(213, 213, 213);
border-radius:10px;
background-color: rgb(255, 255, 255);
}

/*text edit*/
QTextEdit {
border:1px solid rgb(213, 213, 213);
border-radius:10px;
background-color: rgb(255, 255, 255);
}

/*tab widget*/
QTabWidget::pane{
border:none;
}
QTabWidget::tab-bar{
left:10px
}
QTabBar::tab{
background-color: rgb(210, 232, 254);
}
QTabBar::tab:pressed {
 background-color: rgba(85, 170, 255, 200);
}

